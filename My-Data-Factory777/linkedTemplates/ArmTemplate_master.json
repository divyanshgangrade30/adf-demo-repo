{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "My-Data-Factory777"
		},
		"ADLSConnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSConnection'"
		},
		"MongoDbAtlas1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'MongoDbAtlas1'"
		},
		"ADLSConnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://myystorage7770.dfs.core.windows.net/"
		},
		"MongoDbAtlas1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "database7777"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "My-Data-Factory777_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSConnection_accountKey": {
						"value": "[parameters('ADLSConnection_accountKey')]"
					},
					"MongoDbAtlas1_connectionString": {
						"value": "[parameters('MongoDbAtlas1_connectionString')]"
					},
					"ADLSConnection_properties_typeProperties_url": {
						"value": "[parameters('ADLSConnection_properties_typeProperties_url')]"
					},
					"MongoDbAtlas1_properties_typeProperties_database": {
						"value": "[parameters('MongoDbAtlas1_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}